# Environment Variables for Admin Testing
# Copy this file to .env and set your actual values

# API Configuration
BASE_URL=https://cryptouniverse.onrender.com/api/v1

# Admin Credentials (for testing only)
# DO NOT commit real credentials to source control
ADMIN_EMAIL=<YOUR_ADMIN_EMAIL>
ADMIN_PASSWORD=<YOUR_ADMIN_PASSWORD>

# Admin Override Protection
# Set to 'true' to enable admin override actions
ADMIN_OVERRIDE_ENABLED=false

# Confirmation for Admin Actions
# Set to 'true' to confirm admin actions are intentional
CONFIRM_ADMIN_ACTIONS=false