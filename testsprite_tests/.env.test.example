# TestSprite Test Environment Configuration - EXAMPLE FILE
# Copy this to .env.test and replace with actual values
BASE_URL=https://your-app.onrender.com
API_BASE_URL=https://your-app.onrender.com/api/v1
ENVIRONMENT=production
TEST_MODE=true

# Test User Credentials (for automated testing)
# Replace with actual test credentials
TEST_ADMIN_EMAIL=admin@example.com
TEST_ADMIN_PASSWORD=your_admin_password_here
TEST_USER_EMAIL=testuser@example.com
TEST_USER_PASSWORD=your_test_user_password_here

# Test Configuration
TEST_TIMEOUT=30000
TEST_RETRY_COUNT=3
TEST_PARALLEL=false
TEST_VERBOSE=true

# API Testing
TEST_API_KEY=test_api_key_here
TEST_SECRET_KEY=test_secret_key_here

# Deployment Info
DEPLOYMENT_TYPE=render
DEPLOYMENT_URL=https://cryptouniverse.onrender.com
FRONTEND_URL=https://cryptouniverse-frontend.onrender.com